{
    "name": "esoftslimited/yii2-dashboard",
    "description": "Yii2 Dashboard is a module that allows you to create and manage dashboards using custom layouts and panels.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Mwangi",
            "email": "emwangi.g@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "esoftslimited\\dashboard\\": ""
        }
    },
    "extra": {
        "bootstrap": "esoftslimited\\dashboard\\Bootstrap",        
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
